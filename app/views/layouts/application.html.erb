<!DOCTYPE html>
<html>
  <head>
    <title>SurvivorApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
   <nav class="navbar navbar-toggleable-md navbar-light bg-faded" role="navigation">
      <div class="navbar-header my-2">
        <%= link_to "Home", root_path, class: "hvr-rectangle-out navbar-brand" %>
        <%= link_to "Register for League", new_user_path, class: "hvr-rectangle-out register" %>
      </div>
    </nav>
    <%= yield %>
  </body>
</html>
